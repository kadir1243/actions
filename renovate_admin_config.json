{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "onboardingConfig": {
        "$schema": "https://docs.renovatebot.com/renovate-schema.json",
        "extends": ["github>neoforged/actions:renovate_preset"]
    },
    "onboardingConfigFileName": ".github/renovate.json",
    "repositoryCache": "enabled",
    "cacheDir": "/renovatecache",
    "dependencyDashboardFooter": "- [ ] Click to force Renovate to run again on this repository",
    "username": "neoforged-renovate[bot]",
    "httpCacheTtlDays": 0,
    "hostRules": [
        {
          "matchHost": "api.github.com",
          "enableHttp2": true,
          "timeout": 10000
        }
    ]
}